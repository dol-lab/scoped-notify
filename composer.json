{
	"name": "dol-lab/scoped-notify",
	"description": "",
	"type": "wordpress-plugin",
	"license": "GPL-2.0-or-later",
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/dol-lab/custom-table-manager"
		}
	],
	"support": {
		"source" : "https://github.com/dol-lab/scoped-notify",
		"issues" : "https://github.com/dol-lab/scoped-notify/issues"
	},
	"require": {
		"dol-lab/custom-table-manager": "1.*",
		"php": ">=8.1",
		"psr/log": "^3.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.6",
		"squizlabs/php_codesniffer": "^3.12",
		"wp-cli/wp-cli": "2.*",
		"wp-coding-standards/wpcs": "^3.1",
		"yoast/wp-test-utils": "^1.2"
	},
	"scripts": {
		"test": "phpunit"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		},
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"Scoped_Notify\\": "src/"
		}
	}
}
